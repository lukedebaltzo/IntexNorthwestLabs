@model IEnumerable<IntexNorthwestLabs.Models.Test_Results>

@{
    ViewBag.Title = "Order Details";
}

<h2>Order Details</h2>



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AssayName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompoundName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestResultQuantitative)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestResultQualitative)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AssayName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompoundName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestResultQuantitative)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestResultQualitative)
            </td>
        </tr>
    }

</table>
