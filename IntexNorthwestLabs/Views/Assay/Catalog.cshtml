@model IEnumerable<IntexNorthwestLabs.Models.Assay>

@{
    ViewBag.Title = "Catalog";
}

<h2>Catalog</h2>


<table class="table">
    <tr>
        <!--
    <th>
        @Html.DisplayNameFor(model => model.AssayCode)
    </th>-->
        <th>
            @Html.DisplayNameFor(model => model.AssayName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssayDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssayLiteratureRefs)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <!--
    <td>
        @Html.DisplayFor(modelItem => item.AssayCode)
    </td>-->
        <td>
            @Html.DisplayFor(modelItem => item.AssayName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AssayDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AssayLiteratureRefs)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { iCode=item.AssayCode })
        </td>
    </tr>
}

</table>
