@model IEnumerable<IntexNorthwestLabs.Models.Assay_Catalog>

@{
    ViewBag.Title = "Index";
}

<h2>Assay Catalog</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AssayType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssayName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssayDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssayLiteratureRefs)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestDuration)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RequiredTest)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompoundName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaterialName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AssayType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AssayName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AssayDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AssayLiteratureRefs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestDuration)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RequiredTest)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompoundName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaterialName)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.AssayCode })
            </td>
        </tr>
    }

</table>

